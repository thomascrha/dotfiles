# vim: set ft=swayconfig ts=4 sw=4:
# Left
set $first "Dell Inc. DELL U3023E 66YJ4H3"
# Right
set $second "Dell Inc. DELL U3023E 40G15H3"

output $first {
    mode  2561x1600@59.972Hz
    # pos 2560 0
    pos 0 0
    transform normal
    scale 1.0
    scale_filter nearest
    adaptive_sync on
    dpms on
    bg /home/tcrha/.config/sway/images/wallpaper.png fill
}

# Right
output $second {
    mode  2560x1600@59.972Hz
    # pos 0 0
    pos 2560 0
    transform normal
    scale 1.0
    scale_filter nearest
    adaptive_sync on
    dpms on
    bg /home/tcrha/.config/sway/images/wallpaper.png fill
}

bindsym $mod+Shift+l output $first toggle
bindsym $mod+Shift+r output $second toggle


workspace 10 output $first
workspace 11 output $second
workspace 20 output $first
workspace 21 output $second
workspace 30 output $first
workspace 31 output $second
workspace 40 output $first
workspace 41 output $second
workspace 50 output $first
workspace 51 output $second
workspace 60 output $first
workspace 61 output $second
workspace 70 output $first
workspace 71 output $second
workspace 80 output $first
workspace 81 output $second
workspace 90 output $first
workspace 91 output $second
workspace 100 output $first
workspace 101 output $second

